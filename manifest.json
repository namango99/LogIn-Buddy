{
    "manifest_version": 2,
    "name": "LogIn Budy",
    "version": "0.0.1",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {}

}
